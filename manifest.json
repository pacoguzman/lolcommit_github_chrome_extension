{
  "name": "LolCommits Github Chrome Extension",
  "version": "0.1.1",
  "manifest_version": 1,
  "description": "Add LolCommits images in Github",
  "browser_action": {
    "default_icon": "icons/github-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["scripts/inject.js"],
      "run_at": "document_end"
    }
  ]
}
